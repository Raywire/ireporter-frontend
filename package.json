{
  "name": "ireporter",
  "version": "1.0.0",
  "description": "An app for reporting corruption in the country",
  "main": "index.js",
  "engines": {
    "npm": "6.7.0",
    "node": "10.15.3"
  },
  "scripts": {
    "start": "server.js",
    "dev-start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "test": "jest",
    "heroku-postbuild": "webpack -p"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raywire/ireporter-frontend.git"
  },
  "keywords": [
    "corruption",
    "ireporter"
  ],
  "author": "Ryan Wire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Raywire/ireporter-frontend/issues"
  },
  "homepage": "https://github.com/Raywire/ireporter-frontend#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "express": "^4.17.0",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.0.0",
    "moment": "^2.24.0",
    "node": "^12.3.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "smart-truncate": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "html-webpack-plugin": "^3.2.0"
  }
}
